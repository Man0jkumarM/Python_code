print("Welcome to BMI calculator!")
height=float(input("Enter your height:\n"))
weight=float(input("Enter your weight:\n"))
new_height=height/100
bmi=weight/(new_height**2)
bmi=round(bmi,1)
print(bmi)
if bmi<18:
    print("underweight")
elif bmi<25:
    print("normal weight")
elif bmi<30:
    print("overweight")
elif bmi<35:
    print("obese")
else:
    print("clinically obese")

